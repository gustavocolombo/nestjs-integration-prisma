// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id        String @id @default(uuid())
  firstname String
  lastname  String
  email String @unique
  password String
  cellphone String

  @@map("users")
}

model Library{
  id String @id @default(uuid())
  books Int @default(0)
  name String
  latitude Float
  longitue Float
  cellphone String
  
  @@map("libraries")
}